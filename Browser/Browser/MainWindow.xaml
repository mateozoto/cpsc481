<Window x:Class="Browser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Meow Cats Playground Web Browser" Height="640" Width="1014.686">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="videos" Content="Videos" HorizontalAlignment="Left" Margin="0,216.06,0,0" VerticalAlignment="Top" Width="60" Height="85"/>
        <Button x:Name="music" Content="Music" HorizontalAlignment="Left" Margin="0,301.06,0,0" VerticalAlignment="Top" Width="60" Height="85"/>
        <Button x:Name="learning" Content="Learning" HorizontalAlignment="Left" Margin="0,386.06,0,0" VerticalAlignment="Top" Width="60" Height="85"/>
        <Button x:Name="games" Content="Games" HorizontalAlignment="Left" Margin="0,131.06,0,0" VerticalAlignment="Top" Width="60" Height="85"/>
        <TextBlock x:Name="welcome" Margin="216.619,169.803,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="46.257" HorizontalAlignment="Left" Width="159.435"><Run FontSize="29.333" Text="Welcome!"/><LineBreak/><LineBreak/><Run/><LineBreak/><Run/><LineBreak/><Run/><LineBreak/><Run/></TextBlock>
        <TextBlock x:Name="message1" Height="46.57" Margin="288.47,254.49,0,0" TextWrapping="Wrap" Text="No currently open tabs." VerticalAlignment="Top" FontSize="18.667" HorizontalAlignment="Left" Width="387.194"/>
        <TextBox x:Name="urlbar" HorizontalAlignment="Left" Margin="119.742,0,0,18.239" TextWrapping="Wrap" Width="782.619" FontSize="33.333" Height="50.942" VerticalAlignment="Bottom"/>
        <Button x:Name="go" Content="Go" Margin="859.293,0,0,18.239" HorizontalAlignment="Left" Width="49.719" Height="50.942" VerticalAlignment="Bottom"/>
        <Rectangle x:Name="bookmarksrectangle" Fill="#FFF4F4F5" Height="42.189" VerticalAlignment="Top"/>
        <TextBlock x:Name="nobookmarks" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="No bookmarks" VerticalAlignment="Top" Height="19.951" Width="119.291" FontSize="16"/>
        <Button x:Name="home" Content="home" HorizontalAlignment="Left" Margin="26.611,0,0,18.239" VerticalAlignment="Bottom" Width="53.347" Height="50.942" BorderThickness="0" Background="#FF174CFF"/>
        <Button x:Name="settings" Content="Settings" HorizontalAlignment="Left" Height="50.942" Margin="928.698,0,0,18.239" VerticalAlignment="Bottom" Width="53.347" RenderTransformOrigin="0.949,0.522" BorderThickness="0" Background="#FF28B1FF"/>
        <Rectangle x:Name="recentplacesrectangle" Fill="#FFF4F4F5" Height="360.623" VerticalAlignment="Top" Margin="0,110.437,0,0" HorizontalAlignment="Right" Width="60"/>
        <TextBlock x:Name="norecent" Margin="0,193.83,7.338,226.244" TextWrapping="Wrap" TextAlignment="Center" HorizontalAlignment="Right" Width="43.948"><Run/><LineBreak/><Run/><LineBreak/><Run Text="No"/><LineBreak/><Run/><LineBreak/><Run Text=""/><LineBreak/><Run Text="Recent"/><LineBreak/><LineBreak/><Run/><LineBreak/><Run Text="Places"/></TextBlock>

    </Grid>
</Window>
